@model List<Agenda.Service.Dto.ClienteDto>

<div container>
    <table class="table">
        <thead>
            <tr>
                <th scope="col">Id</th>
                <th scope="col">Nome</th>
                <th scope="col">Email</th>
                <th scope="col">Telefone</th>
                <th scope="col">CPF</th>
                <th scope="col">Data de Nascimento</th>                
                <th scope="col">Ação</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <th scope="row">@item.Id</th>
                    <td>@item.Nome</td>
                    <td>@item.Email </td>
                    <td>@item.Telefone</td>
                    <td>@item.Cpf</td>
                    <td>@item.DataNascimento</td>                   
                    <td>
                        <button type="button" class="btn btn-primary btn-sm"><i class="fa fa-eye" aria-hidden="true"></i>                      
                    </td>
                </tr>
            }

            </tbody>
    </table>
</div>